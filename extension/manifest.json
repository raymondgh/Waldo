{
	"manifest_version": 1,
	"name": "Waldo",
	"version": "1.0",
	"description": "Automatically recommend similar bookmarks using IDOL OnDemand",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["js/jquery-2.0.3.min.js","keys.js","main.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": ["js/jquery-2.0.3.min.map"] ,
	"permissions":["tabs","<all_urls>","bookmarks"]
}